{% extends 'bar.html.twig' %}

{% block body %}
    <h1>Admin Profile</h1>

    <h2>Welcome, {{ user.email }}</h2>
    <p>Email: {{ email }}</p>

    <h3>All Users</h3>
    <ul>
        {% for user in users %}
            <li>
                <a href="{{ path('admin_user_notes', {'id': user.id}) }}">{{ user.email }}</a>
            </li>
        {% endfor %}
    </ul>
{% endblock %}
